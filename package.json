{
	"name": "metadescription",
	"displayName": "MetaDescription",
	"description": "metadescription allows you to add a description to any folder or file in your VSCode workspace. Simply right click any folder or file in the Explorer, and click \"Edit Description\". You will then be provided with a markdown file that is associated with this file.",
	"version": "1.0.0",
	"publisher": "GregoryGlatzer",
	"icon": "https://raw.githubusercontent.com/g1776/metadescription/master/metadescription%20logo.png",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/g1776/metadescription.git"
	},
	"activationEvents": [
		"onCommand:metadescription.main"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "metadescription.main",
				"title": "Edit Description"
			}
		],
		"menus": {
			"explorer/context": [
				{
					
					"command": "metadescription.main",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
	"dependencies": {
		"temp": "^0.9.4"
	}
}
